[
  {
    "clean": ["~"]
  },
  {
    "link": {
      "~/.vimrc": "vimrc",
      "~/.vim": "vim/",
      "~/.bin": "bin/",
      "~/.compton.conf": "compton.conf",
      "~/.dircolors.256dark": "dircolors.256dark",
      "~/.gitconfig": "gitconfig",
      "~/.gitignore_global": "gitignore_global",
      "~/.xinitrc": "xinitrc",
      "~/.config/bspwm/bspwmrc": "bspwm/bspwmrc.dualscreen",
      "~/.config/sxhkd/sxhkdrc": "sxhkd/sxhkdrc.dualscreen"
    }
  },
  {
    "shell": [
      ["git submodule update --init --recursive", "Installing submodules"]
    ]
  }
]
